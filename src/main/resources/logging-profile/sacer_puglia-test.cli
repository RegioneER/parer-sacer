batch
echo "Logging-Profile: SACER (Puglia Test)" 
/profile=puglia-test/subsystem=logging/logging-profile=SACER:add
echo "Log su file"
/profile=puglia-test/subsystem=logging/logging-profile=SACER/periodic-rotating-file-handler=sacer_handler:add(file={"path"=>"sacer.log","relative-to"=> "jboss.server.log.dir"}, suffix=".yyyy-MM-dd",append="true" )
/profile=puglia-test/subsystem=logging/logging-profile=SACER/periodic-rotating-file-handler=sacer_handler:write-attribute(name="level", value="DEBUG")
/profile=puglia-test/subsystem=logging/logging-profile=SACER/periodic-rotating-file-handler=sacer_handler:write-attribute(name="formatter",value="%d{yyyy-MM-dd HH:mm:ss,SSS} %-5p [%c] (%t) %X{log_uuid} %s%E%n")
echo "Logger specifici"
/profile=puglia-test/subsystem=logging/logging-profile=SACER/logger=org.springframework:add
/profile=puglia-test/subsystem=logging/logging-profile=SACER/logger=org.springframework:write-attribute(name="use-parent-handlers", value="true")
/profile=puglia-test/subsystem=logging/logging-profile=SACER/logger=org.springframework:write-attribute(name="level", value="ERROR")
/profile=puglia-test/subsystem=logging/logging-profile=SACER/logger=org.opensaml:add
/profile=puglia-test/subsystem=logging/logging-profile=SACER/logger=org.opensaml:write-attribute(name="use-parent-handlers", value="true")
/profile=puglia-test/subsystem=logging/logging-profile=SACER/logger=org.opensaml:write-attribute(name="level", value="ERROR")
/profile=puglia-test/subsystem=logging/logging-profile=SACER/logger=es.mityc:add
/profile=puglia-test/subsystem=logging/logging-profile=SACER/logger=es.mityc:write-attribute(name="use-parent-handlers", value="true")
/profile=puglia-test/subsystem=logging/logging-profile=SACER/logger=es.mityc:write-attribute(name="level", value="INFO")
/profile=puglia-test/subsystem=logging/logging-profile=SACER/logger=it.eng.crypto:add
