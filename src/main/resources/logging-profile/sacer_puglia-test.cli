/profile=puglia-test/subsystem=logging/logging-profile=SACER/logger=it.eng.parer.volume:write-attribute(name="level", value="INFO")
/profile=puglia-test/subsystem=logging/logging-profile=SACER/logger=it.eng.parer.ws:add
