/profile=puglia-test/subsystem=logging/logging-profile=SACER/root-logger=ROOT:add(handlers=[sacer_handler])
/profile=puglia-test/subsystem=logging/logging-profile=SACER/root-logger=ROOT:write-attribute(name ="level",value="INFO")
